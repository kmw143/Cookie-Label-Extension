{
  "manifest_version": 3,
  "name": "Cookie Nutrition Label",
  "description": "Displays detailed cookie information for websites",
  "version": "1.1",
  "permissions": ["cookies", "storage", "activeTab", "scripting"],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "index.html",
    "default_icon": "hello_extensions.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ]
}









